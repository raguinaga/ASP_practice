@page
@model ListBinding.Pages.ShowRates

@{
    ViewData["Title"] = "The rates";
}

<!DOCTYPE html>

<html>
<head>
</head>
<body>
<h1>Exchange rates</h1>
<dl class="dl-horizontal">
    @foreach (var item in Model.SelectedCurrencies) 
    {
        <dt>@item.Key</dt>
        <dd>@item.Value</dd>
    }
</dl>
</body>
</html>